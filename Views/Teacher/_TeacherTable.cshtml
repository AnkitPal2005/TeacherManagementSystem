@model List<TeacherManagementSystem.Models.Teacher>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Subject</th>
        <th>Action</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.SubjectName</td>
            <td>
                <button class="btn btn-warning btn-sm btn-edit" data-id="@item.Id">Edit</button>
                <button class="btn btn-danger btn-sm btn-teacher-delete" data-id="@item.Id">Delete</button>
            </td>
        </tr>
    }
</table>